  "scripts": {
    "start:client": "webpack-dev-server --mode development --config configs/webpack/webpack.client.config",
    "start:server": "webpack-dev-server --mode development --config configs/webpack/webpack.server.config",
    "start": "concurrently \"npm:start:client\" \"npm:start:server\"",
    "build:dev:client": "webpack --mode development --config configs/webpack/webpack.client.config",
    "build:dev:server": "webpack --mode development --config configs/webpack/webpack.server.config",
    "build:dev": "npm run build:dev:client && npm run build:dev:server",
    "build:client": "webpack --mode production --config configs/webpack/webpack.client.config",
    "build:server": "webpack --mode production --config configs/webpack/webpack.server.config",
    "build": "npm run build:client && npm run build:server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
